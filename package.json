{
  "name": "angular-seed-protractor-tests",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "node build/clean.js",
    "compile": "node ./node_modules/typescript/bin/tsc -p src --outDir target",
    "start": "npm run-script clean && npm run-script compile && node ./node_modules/protractor/bin/protractor protractor.conf.js",
    "update": "node ./node_modules/protractor/bin/webdriver-manager update"
  },
  "devDependencies": {
    "typescript": "~2.7.2",
    "protractor": "~5.3.0",
    "selenium": "~2.20.0",
    "@types/node": "~9.4.6",
    "@types/protractor": "~4.0.0",
    "@types/jasminewd2": "~2.0.3",
    "del": "~3.0.0",
    "protractor-jasmine2-html-reporter": "~0.0.7"
  }
}
